package evision;

public class SwapTwoNumbersWihThirsVariable {
	public static void main(String[] args) {
		
		int a  = 12;
		int b = 85;
		int c;
		
		System.out.println("Before Swapping A & B "+a+" "+b);
		c = a + b;
		a = c - b;
		b = c - a;
		System.out.println("After Sapping A & B "+a+" "+b);
	}
}
