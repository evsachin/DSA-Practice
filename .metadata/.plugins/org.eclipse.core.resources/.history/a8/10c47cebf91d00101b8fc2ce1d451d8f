package evision;

public class Sample {

	public static void main(String[] args) {
		
		System.out.println("Hello World");
		System.out.println(10*2);
		
		//java data types
		int num = 45;
		float myFolatNum = 45.6f;
		char myLetter = 'D';
		boolean myBool = true;
		String myText = "Hello";
		
		
		String name = "Sachin";
		String sirname = "Kolhe";
		
		System.out.println(name+sirname);
		System.out.println(name.concat(sirname));
		
	}

}
 	